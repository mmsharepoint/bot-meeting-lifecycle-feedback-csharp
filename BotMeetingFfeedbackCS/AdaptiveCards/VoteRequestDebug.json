{
  "type": "AdaptiveCard",
  "schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4",
  "refresh": {
    "action": {
      "type": "Action.Execute",
      "title": "Refresh",
      "verb": "alreadyVoted",
      "data": {
        "feedback": {
          "meetingID": "${meetingID}",
          "votedPersons": [
            "${votedPersons}"
          ],
          "votes1": "${votes1}",
          "votes2": "${votes2}",
          "votes3": "${votes3}",
          "votes4": "${votes4}",
          "votes5": "${votes5}"
        }
      }
    },
    "userIds": "${votedPersons}"
  },
  "body": [
    {
      "type": "TextBlock",
      "text": "How did you like the meeting?",
      "wrap": true,
      "horizontalAlignment": "Center"
    },
    {
      "type": "ActionSet",
      "actions": [
        {
          "type": "Action.Execute",
          "title": " ",
          "verb": "vote_1",
          "iconUrl": "${_hosturl}/images/1.png",
          "data": {
            "feedback": {
              "meetingID": "${meetingID}",
              "votedPersons": [
                "${votedPersons}"
              ],
              "votes1": "${votes1}",
              "votes2": "${votes2}",
              "votes3": "${votes3}",
              "votes4": "${votes4}",
              "votes5": "${votes5}"
            }
          }
        },
        {
          "type": "Action.Execute",
          "title": " ",
          "verb": "vote_2",
          "iconUrl": "${_hosturl}/images/2.png",
          "data": {
            "feedback": {
              "meetingID": "${meetingID}",
              "votedPersons": [
                "${votedPersons}"
              ],
              "votes1": "${votes1}",
              "votes2": "${votes2}",
              "votes3": "${votes3}",
              "votes4": "${votes4}",
              "votes5": "${votes5}"
            }
          }
        },
        {
          "type": "Action.Execute",
          "title": " ",
          "verb": "vote_3",
          "iconUrl": "${_hosturl}/images/3.png",
          "data": {
            "feedback": {
              "meetingID": "${meetingID}",
              "votedPersons": [
                "${votedPersons}"
              ],
              "votes1": "${votes1}",
              "votes2": "${votes2}",
              "votes3": "${votes3}",
              "votes4": "${votes4}",
              "votes5": "${votes5}"
            }
          }
        }
      ]
    }
  ]
}